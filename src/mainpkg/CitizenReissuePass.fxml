                  <Font name="Calibri" size="20.0" />
               </font>
               <padding>
                  <Insets left="15.0" right="15.0" />
               </padding>
            </Label>
            <ComboBox fx:id="passReissueDurationComboBox" prefHeight="32.0" prefWidth="227.0" promptText="Duration" style="-fx-background-color: #BDFCC9;" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <Label fx:id="confirmationLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="19.0" layoutY="183.0" prefHeight="38.0" prefWidth="463.0" style="-fx-text-fill: red;" text="confirmationLabel">
         <font>
            <Font name="Calibri" size="20.0" />
         </font>
         <padding>
            <Insets left="15.0" right="15.0" />
         </padding>
      </Label>
      <Button fx:id="applyPassButton" layoutX="204.0" layoutY="236.0" mnemonicParsing="false" onAction="#applyPassButtonOnClick" prefHeight="30.0" prefWidth="92.0" style="-fx-background-color: #3B5323; -fx-text-fill: #5DFC0A;" text="Apply" textFill="WHITE">
         <font>
            <Font name="Calibri Bold" size="14.0" />
         </font>
      </Button>
   </children>
